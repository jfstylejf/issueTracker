<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.fudan.cloneservice.mapper.PackageNameMapper">

    <!--<select id="selectPackageNameSet"-->

    <insert id="insertPackageNameSetByRepoIdAndCommitId" parameterType="Stirng">
        insert into ce_package_info set uuid = #{p_uuid},
                                          repo_id = #{p_repo_id},
                                          commit_id = #{p_commit_id},
                                          package_name = #{p_package_name}
    </insert>
</mapper>