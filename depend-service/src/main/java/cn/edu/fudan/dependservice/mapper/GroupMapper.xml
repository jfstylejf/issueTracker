<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace：命名空间，用于隔离sql，还有一个很重要的作用，后面会讲 -->
<mapper namespace="cn.edu.fudan.dependservice.mapper.GroupMapper">

    <select id="getAllGroups" resultType="cn.edu.fudan.dependservice.domain.Group">
    select repo_uuid,group_id,file,depend_on,depend_details from dependecy_detail
    </select>

    <insert id="add">
        insert into dependency(commit_id,repo_uuid,group_id,cycle_num)
        values(#{commit_id},#{repo_uuid},#{group_id},#{cycle_num})
    </insert>


